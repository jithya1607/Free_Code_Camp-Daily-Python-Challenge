def squares_with_three(n):
    
        if not isinstance(n,int):
            raise TypeError('Value must be an integer!')

        if n<1 or n>10000:
            raise ValueError('Value must be within range!')

        a=0
        for i in range(1,n+1):
            if '3' in str(i**2):
                a=a+1

        return a
