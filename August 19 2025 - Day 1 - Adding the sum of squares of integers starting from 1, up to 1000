def sum_of_squares(n):
   try:
       if not isinstance (n,int):
           raise TypeError('Value must be an integer!')       

       if n<0 or n>1000:
            raise ValueError('Value out of range!')

       if n<=1000 and n>0:
           a=0
           for i in range(1,n+1):
               a=a+i**2
       return a

   except (TypeError,ValueError) as e:
        print('Error:',e)
